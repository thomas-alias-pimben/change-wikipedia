{
  "manifest_version": 3,
  "name": "wikichange logo",
  "version": "1.0",
  "description": "change wikipedia logo",

 
  
 "web_accessible_resources": [
    {
      "resources": ["/images/*"],
      "matches": ["https://fr.wikipedia.org/*"]
    }],

  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://fr.wikipedia.org/*"
      ]
    }
  ]
}